<div class="inner questions_answers_page">

  <div class="sub_heading_wrapper search_page_header">
    <h1>Search Results</h1>
    <%= render 'quick_nav' %>
    </div>


  <div class="page">
    <div class="speciality_search_page_wrapper">
      <div id="tabs_single_page">
        <% unless @videos.empty? %>

              <p class="search_tab_sub_title">Vidoes that cover this topic:</p>
              <%= render partial: 'dashboards/video_list', locals: {videos: @videos} %>
            </div>

        <% else %>
          <div class="no_search_results_wrapper">
            <p class="no_search_results_found_title">Sorry nothing found for this search</p>
          </div>
        <% end %>
    </div>
    <div class="clear"></div>
  </div>
</div>
