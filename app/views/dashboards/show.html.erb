<div class="inner">
  <div class="sub_heading_wrapper">
    <h1>Dashboard</h1>
    <div class="sub_heading_dashboard_info">
      <p><strong>Welcome back <%= current_user.name %></strong></p>
      <p>Your current subscription is active &amp; valid until the 28th March 2014</p>
    </div>
  </div>

  <div class="page dashboard_page">

    <div id="tabs">
      <ul>
        <li><a href="#tabs-1" title="" class="lectures_tab"><p>Lectures</p><p class="icon"></p></a></li>
        <li><a href="#tabs-2" title="" class="recommended_lectures_tab"><p>Recommended Lectures</p><p class="icon"></p></a></li>
        <li><a href="#tabs-3" title="" class="stats_tab"><p>Stats</p><p class="icon"></p></a></li>
      </ul>

      <div id="tabs_container">
        <div id="tabs-1" class="tab_box">
          <div class="inner_page_padding">
            <div class="tab_title">New Lectures</div>
            <div class="lecture_list">
              <%= render 'video_list' %>
            </div>
          </div>
        </div>

        <div id="tabs-2" class="tab_box">
          <div class="inner_page_padding">
            <p class="recommended_tab_sub_title">The below lectures are recommended to you because you have either not watched it, it's part watched or you did not reach a pass grade of atleast 78%</p>
            <%= render 'video_list' %>
          </div>
        </div>

        <div id="tabs-3" class="tab_box">
          <div class="stats_chart_wrapper">
            <div class="stats_chart">
                <div class="chart-wrapper">
                  <div id="chart"></div>
                </div>
            </div>
          </div>
          <%= render 'stats' %>
        </div>
      </div>
  </div>

  <div class="clear"></div>
</div>

</div>
