<div class="inner dashboard_page">
  <div>
    <div class="sub_heading_wrapper">
      <h1>Dashboard</h1>
      <div class="sub_heading_button_set">
        <%= render 'quick_nav' %>
      </div>

      <div class="sub_heading_dashboard_info">
        <p><strong>Welcome back <%= current_user %></strong></p>
      </div>
    </div>
  </div>

  <div class="page dashboard_page">

    <div id="tabs">
      <ul>
        <li><a href="#tabs-1" title="" class="stats_tab"><p>Your Progress</p><p class="icon"></p></a></li>
      </ul>

      <div id="tabs_container">
        <div id="tabs-1" class="tab_box">
          <div class="stats_chart_wrapper" id="tour_step_one">
            <%= render 'stats' %>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="clear"></div>

</div>

<%= render "trial_user_modal" %>
