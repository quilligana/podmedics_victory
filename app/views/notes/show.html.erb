<div class="inner questions_answers_page">

  <div class="sub_heading_wrapper search_page_header">
    <h1><%= link_to @notes.cached_title, @notes.cached_noteable %></h1>
    <div class="sub_heading_button_set">
      <%= render 'notes_nav' %>
    </div>
  </div>

  <div class="page">
    <div class="note_right_column_full_note show_note_page_inner" id="full_note_one">
      <p><%= sanitize(@notes.content) %></p>
    </div>

    <div class="right_column_save_note_button show_note_page_save_button_wrapper">
      <div class="right_column_save_note_button_wrapper">
        <a href="javascript:;" class="podmedcis_button grey" id="save_new_note_button">Edit Note</a>
      </div>
    </div>

    <div class="video_page_notes_column">
      <div class="video_page_notes_column_title">
        <strong>Edit notes</strong>
      </div>
      <div class="video_page_notes_column_divide"></div>
      <%= render partial: 'form' %>
    </div>
  </div>

  <div class="clear"></div>
  </div>
</div>
