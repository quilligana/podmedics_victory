<div class="inner questions_answers_page">

    <div class="sub_heading_wrapper search_page_header">
      <h1>Notes</h1>
      <%= render 'notes_nav' %>
    </div>


  <div class="page">
    <% unless @notes.empty? %>
      <%= render partial: 'note', collection: @notes %>
    <% else %>
      <div class="no_notes_found_notice">No notes found for this specialty</div>
    <% end %>
  </div>

  <div class="clear"></div>
  </div>
</div>
