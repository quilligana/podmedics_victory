<div class="inner questions_answers_page">

  <div class="sub_heading_wrapper search_page_header">
    <h1><%= "Notes" %></h1>
    <%= link_to 'Print Notes', url_for(:format => "pdf") %>
    <%= render 'notes_nav' %>
  </div>


  <div class="page">
    <% unless @notes.empty? %>
      <% @notes.each do |category| %>
        <% puts category.inspect %>
        <h2><%= category.first.first.category.name %></h2>

        <% category.each do |specialty| %>
          <h3><%= specialty.first.specialty.name %></h3>

          <%= render partial: 'note', collection: specialty %>
        <% end %>

        <div class="break"></div>
      <% end %>
    <% else %>
      <div class="no_notes_found_notice">No notes found for this specialty</div>
    <% end %>
  </div>

  <div class="clear"></div>
  </div>
</div>
