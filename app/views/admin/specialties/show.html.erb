<h1 class="page-header">
  <%= @specialty.name %>
</h1>

<div class="row">
  <div class="lg-span-12">
    <table border="0" class="table">
      <thead>
        <tr>
          <th>#ID</th>
          <th>Title</th>
          <th>Position</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @specialty.videos.each do |video| %>
          <tr>
            <td><%= video.id %></td>
            <td><%= video.title %></td>
            <td><%= video.position %></td>
            <td>
              <%= link_to "Show", admin_video_path(video) %>
              <%= link_to "Edit", edit_admin_video_path(video) %>
              <%= link_to "Up", move_up_admin_video_path(video) %>
              <%= link_to "Down", move_down_admin_video_path(video) %>
            </td>
          </tr>
        <% end %>
      </tbody>
      
    </table>
    
  </div>
  
</div>
